<section class="content-header">
    <h1>
        Temporary Car Form
    </h1>
</section>
<section class="content" ng-controller="TemporaryCtrl" ng-init="init()">
    <div class="row">
        <form role="form">
            <div ng-if="TemporaryCtrl.dataTemp.length==null">
                <!-- left column -->
                <div class="col-md-12">
                    <div class="box-tools pull-right" style="margin-top:-50px">
                        <h4>
                            <label for="" class="control-label">Status:</label>
                            <label class="control-label">Draft</label>
                        </h4>
                    </div>
                    <!-- general form elements -->
                    <div class="box box-primary box">
                        <div class="box-header with-border">
                            <h3 class="box-title">Temporary Form Info</h3>
                            <div class="box-tools pull-right">
                                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                                    <i class="fa fa-minus"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <div class="col-md-6">
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Request Number</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="" readonly="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Request Date</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="{{TemporaryCtrl.temp_date}}" readonly="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Coordinator</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="{{TemporaryCtrl.coordinator}}" readonly="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Reason</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" ng-model="TemporaryCtrl.reason">
                                            <option ng-repeat="data in TemporaryCtrl.dataReason" value="{{data.reason_id}}">{{data.reason1}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Reference Number</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.reference" value="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Supply Method</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="Temporary" readonly="">
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="box-header with-border" style="margin-top: -51px">
                                    <h3 class="box-title">Assigned Employee</h3>
                                </div>
                                <div class="box-body" style="margin-top: 10px">
                                    <div class="col-md-4">
                                        <label class="control-label">Employee ID</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.employee_id" ng-click="TemporaryCtrl.openEmployee()">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label" data-target="#employee">Employee Name</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.employee" disabled>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Cost Center</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.cc" disabled>
                                    </div>
                                </div>
                                <div class="box-body" ng-if="TemporaryCtrl.Type=='Benefit'">
                                    <div class="col-md-4">
                                        <label class="control-label">Group Level</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.groupLevel" disabled>
                                    </div>
                                </div>
                                <div class="box-body" ng-if="TemporaryCtrl.Type=='Benefit'">
                                    <div class="col-md-4">
                                        <label class="control-label">Actual Group Level</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" ng-model="TemporaryCtrl.ActualGroupLevel">
                                            <option ng-repeat="kategori in TemporaryCtrl.datakategori" value="{{kategori.value}}">{{kategori.category}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="box-body">
                            <div class="col-md-6">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Vehicle</h3>
                                </div>

                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="Vtype" class="control-label">Vehicle Type</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="Vtype" ng-model="TemporaryCtrl.Type">
                                            <option ng-repeat="data in TemporaryCtrl.dataVtype">{{data.name}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Project</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" ng-model="TemporaryCtrl.isProject" ng-disabled="TemporaryCtrl.Type=='Benefit'">
                                            <option ng-repeat="data in TemporaryCtrl.dataIsProject" value="{{data.value}}">{{data.name}}</option>
                                            <option ng-if="TemporaryCtrl.Type=='Benefit'" value="0" selected>No</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-body" ng-if="TemporaryCtrl.isProject==1&&TemporaryCtrl.Type!='Benefit'">
                                    <div class="col-md-4">
                                        <label class="control-label">Project Name</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" ng-model="TemporaryCtrl.project">
                                            <option ng-repeat="data in TemporaryCtrl.dataProject" value="{{data.project_id}}">{{data.project_name}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-header with-border">
                                    <h3 class="box-title" style="margin-left: -10px">Car Location</h3>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">City</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.kota" id="myModalLabel" ng-click="TemporaryCtrl.openCity()">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Address</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="" ng-model="TemporaryCtrl.alamat" disabled>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="box-tools pull-right">
                                    <a class="btn bg-blue" style="margin-bottom: 15px;margin-top: -10px;margin-right: 25px " ng-click="TemporaryCtrl.selectVehicle()">
                                        <i class="glyphicon glyphicon-check" id="myModalLabel" data-toggle="modal"></i>  Select Vehicle
                                    </a>
                                </div>
                                <div class="box-body">
                                    <table class="table table-striped">
                                        <thead>
                                            <tr>
                                                <th>Manufacturer</th>
                                                <th>Model</th>
                                                <th>Series</th>
                                                <th>Body Type</th>
                                                <th>Color</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>{{TemporaryCtrl.selectedVehicle.manufacturer}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.model}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.series}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.body_type}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.color}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Start Date</label>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-calendar" ng-click="TemporaryCtrl.openCalendar(1)"></span></span>
                                            <input readonly required class="form-control" size="16" type="text" uib-datepicker-popup="dd-MMM-yyyy" close-text="Tutup" is-open="TemporaryCtrl.isCalendarOpened[1]" ng-model="TemporaryCtrl.start_date" ng-click="TemporaryCtrl.openCalendar(1)" style="background-color: #ffffff;" />
                                        </div>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">End Date</label>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group date">
                                            <span class="input-group-addon"><span class="fa fa-calendar" ng-click="TemporaryCtrl.openCalendar(2)"></span></span>
                                            <input readonly required class="form-control" size="16" type="text" uib-datepicker-popup="dd-MMM-yyyy" close-text="Tutup" is-open="TemporaryCtrl.isCalendarOpened[2]" ng-model="TemporaryCtrl.end_date" ng-click="TemporaryCtrl.openCalendar(2)" style="background-color: #ffffff;" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="box-tools pull-right" style="margin-bottom: 20px; margin-right: 20px">
                    <a class="btn bg-blue" style="margin-top:5px;" ng-click="TemporaryCtrl.saveDraft()">
                        <i class="glyphicon glyphicon-floppy-save"></i> Save
                    </a>
                    <a class="btn bg-blue" style="margin-top:5px;" ng-click="TemporaryCtrl.sendDraft()">
                        <i class="glyphicon glyphicon-ok"></i> Send
                    </a>
                </div>
            </div>
            <div ng-if="TemporaryCtrl.dataTemp.length!=null" ng-repeat="temp in TemporaryCtrl.dataTemp">
                <input type="text" style="visibility:hidden" id="tempID" value="{{temp.temporary_id}}" disabled>
                <div class="col-md-12">
                    <div class="box-tools pull-right" style="margin-top:-50px" ng-init="TemporaryCtrl.getStatus(temp.temporary_status_id)">
                        <h4>
                            <label for="" class="control-label">Status:</label>
                            <label class="control-label">{{TemporaryCtrl.statusCSF}}</label>
                        </h4>
                    </div>
                    <div class="box box-primary box">
                        <div class="box-header with-border">
                            <h3 class="box-title">Temporary Form Info</h3>
                            <div class="box-tools pull-right">
                                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                                    <i class="fa fa-minus"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <div class="col-md-6">
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Request Number</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="{{temp.request_number}}" readonly="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Request Date</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="{{TemporaryCtrl.dateFormat(temp.request_date)}}" readonly="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Coordinator</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="{{TemporaryCtrl.coordinator}}" readonly="">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Reason</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="reason" ng-model="TemporaryCtrl.reason" ng-disabled="temp.temporary_status_id!=4">
                                            <option ng-repeat="data in TemporaryCtrl.dataReason" ng-if="data.reason_id!=temp.reason_id" value="{{data.reason_id}}">{{data.reason1}}</option>
                                            <option ng-repeat="data in TemporaryCtrl.dataReason" ng-if="data.reason_id==temp.reason_id" value="{{data.reason_id}}" selected>{{data.reason1}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Reference Number</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="temp.refference_number" ng-disabled="temp.refference_number!=null">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Supply Method</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" value="Temporary" readonly="">
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6" ng-init="TemporaryCtrl.insertEmployee(temp)">
                                <div class="box-header with-border" style="margin-top: -51px">
                                    <h3 class="box-title">Assigned Employee</h3>
                                </div>
                                <div class="box-body" style="margin-top: 10px">
                                    <div class="col-md-4">
                                        <label class="control-label">Employee ID</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.employee_id" ng-click="TemporaryCtrl.openEmployee()" ng-disabled="temp.temporary_status_id!=4||temp.refference_number!=null">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label" data-target="#employee">Employee Name</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.employee" disabled>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Cost Center</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.cc" disabled>
                                    </div>
                                </div>
                                <div class="box-body" ng-if="TemporaryCtrl.Type=='Benefit'">
                                    <div class="col-md-4">
                                        <label class="control-label">Group Level</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.groupLevel" disabled>
                                    </div>
                                </div>
                                <div class="box-body" ng-if="TemporaryCtrl.Type=='Benefit'">
                                    <div class="col-md-4">
                                        <label class="control-label">Actual Group Level</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="actual" ng-model="TemporaryCtrl.ActualGroupLevel" ng-disabled="temp.temporary_status_id!=4||temp.refference_number!=null">
                                            <option ng-repeat="kategori in TemporaryCtrl.datakategori" ng-if="temp.actual_group!=kategori.value" value="{{kategori.value}}">{{kategori.category}}</option>
                                            <option ng-repeat="kategori in TemporaryCtrl.datakategori" ng-if="temp.actual_group==kategori.value" value="{{kategori.value}}" selected>{{kategori.category}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="box-body">
                            <div class="col-md-6">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Vehicle</h3>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="Vtype" class="control-label">Vehicle Type</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="Vtype" ng-model="TemporaryCtrl.Type" ng-disabled="temp.temporary_status_id!=4||temp.refference_number!=null">
                                            <option ng-repeat="data in TemporaryCtrl.dataVtype" ng-if="temp.vehicle_type!=data.name">{{data.name}}</option>
                                            <option ng-repeat="data in TemporaryCtrl.dataVtype" ng-if="temp.vehicle_type==data.name" selected>{{data.name}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Project</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="isProject" ng-model="TemporaryCtrl.isProject" ng-disabled="TemporaryCtrl.Type=='Benefit'||temp.vehicle_type=='Benefit'||temp.temporary_status_id!=4">
                                            <option ng-repeat="data in TemporaryCtrl.dataIsProject" ng-if="temp.is_project!=data.is" value="{{data.value}}">{{data.name}}</option>
                                            <option ng-repeat="data in TemporaryCtrl.dataIsProject" ng-if="temp.is_project==data.is" value="{{data.value}}" selected>{{data.name}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-body" ng-if="(TemporaryCtrl.isProject==1 || temp.is_project==true)&&(TemporaryCtrl.Type!='Benefit'||temp.vehicle_type!='Benefit')">
                                    <div class="col-md-4">
                                        <label class="control-label">Project Name</label>
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="project" ng-model="TemporaryCtrl.project" ng-disabled="temp.temporary_status_id!=4">
                                            <option ng-repeat="data in TemporaryCtrl.dataProject" value="{{data.project_id}}">{{data.project_name}}</option>
                                            <option ng-repeat="data in TemporaryCtrl.dataProject" ng-if="" value="{{data.project_id}}">{{data.project_name}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="box-header with-border">
                                    <h3 class="box-title" style="margin-left: -10px">Car Location</h3>
                                </div>
                                <div class="box-body" ng-init="TemporaryCtrl.getLocation(temp.location_id)">
                                    <div class="col-md-4">
                                        <label class="control-label">City</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="TemporaryCtrl.kota" id="myModalLabel" ng-click="TemporaryCtrl.openCity()" ng-disabled="temp.temporary_status_id!=4">
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">Address</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="" ng-model="TemporaryCtrl.alamat" disabled>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="box-tools pull-right">
                                    <button class="btn bg-blue" style="margin-bottom: 15px;margin-top: -10px;margin-right: 25px " ng-click="TemporaryCtrl.selectVehicle()" ng-disabled="temp.temporary_status_id!=4">
                                        <i class="glyphicon glyphicon-check" id="myModalLabel" data-toggle="modal"></i>  Select Vehicle
                                    </button>
                                </div>
                                <div class="box-body">
                                    <table class="table table-striped" ng-init="TemporaryCtrl.insertVehicle(temp)">
                                        <thead>
                                            <tr>
                                                <th>Manufacturer</th>
                                                <th>Model</th>
                                                <th>Series</th>
                                                <th>Body Type</th>
                                                <th>Color</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>{{TemporaryCtrl.selectedVehicle.manufacturer}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.model}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.series}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.body_type}}</td>
                                                <td>{{TemporaryCtrl.selectedVehicle.color}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="box-body" ng-init="TemporaryCtrl.setDate(temp)">
                                    <div class="col-md-4">
                                        <label class="control-label">Start Date</label>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-calendar" ng-click="TemporaryCtrl.openCalendar(1)"></span></span>
                                            <input required class="form-control" size="16" type="text" uib-datepicker-popup="dd-MMM-yyyy" close-text="Tutup" is-open="TemporaryCtrl.isCalendarOpened[1]" ng-model="TemporaryCtrl.start_date" ng-click="TemporaryCtrl.openCalendar(1)" ng-disabled="temp.temporary_status_id!=4||temp.refference_number!=null">
                                        </div>
                                    </div>
                                </div>
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label class="control-label">End Date</label>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group date">
                                            <span class="input-group-addon"><span class="fa fa-calendar" ng-click="TemporaryCtrl.openCalendar(2)"></span></span>
                                            <input required class="form-control" size="16" type="text" uib-datepicker-popup="dd-MMM-yyyy" close-text="Tutup" is-open="TemporaryCtrl.isCalendarOpened[2]" ng-model="TemporaryCtrl.end_date" ng-click="TemporaryCtrl.openCalendar(2)" ng-disabled="temp.temporary_status_id!=4||temp.refference_number!=null">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="box box-primary" ng-if="temp.temporary_status_id!=4">
                        <div class="box-header with-border">
                            <h3 class="box-title">Confirmation</h3>
                            <div class="box-tools pull-right">
                                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                                    <i class="fa fa-minus"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.box-header -->
                        <!-- form start -->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="box-body">
                                    <div class="col-md-4">
                                        <label for="" class="control-label">Detail Vehicle</label>
                                    </div>
                                    <div class="col-md-8">
                                        <a class="btn btn-default btn-file  bg-blue" ng-click="TemporaryCtrl.uploadDetailVehicle()">
                                            <i class="glyphicon glyphicon-upload" id="myModalLabel2"></i> Upload & View
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
                <div class="box-tools pull-right" style="margin-bottom: 20px; margin-right: 20px">
                    <button class="btn bg-blue" style="margin-top:5px;" ng-click="TemporaryCtrl.cancel(temp.temporary_id)" ng-disabled="temp.temporary_status_id==11">
                        <i class="glyphicon glyphicon-ban-circle"></i> Cancel
                    </button>
