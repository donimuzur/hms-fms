@model FMS.Website.Models.TempItemModel

<table id="tbTempVehicle" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Manufacturer</th>
            <th>Model</th>
            <th>Series</th>
            <th>Body Type</th>
            <th>Color</th>
            <th>Start Period</th>
            <th>End Period</th>
        </tr>
    </thead>
    <tbody id="tb-body-select-vehicle">
        @if (Model.Detail.Models != null)
        {
            <tr>
                @Html.HiddenFor(model => model.Detail.Manufacturer)
                @Html.HiddenFor(model => model.Detail.Models)
                @Html.HiddenFor(model => model.Detail.Series)
                @Html.HiddenFor(model => model.Detail.BodyType)
                @Html.HiddenFor(model => model.Detail.Color)
                @Html.HiddenFor(model => model.Detail.StartPeriod)
                @Html.HiddenFor(model => model.Detail.EndPeriod)
                @Html.HiddenFor(model => model.Detail.CfmIdleId)
                @Html.HiddenFor(model => model.Detail.CarGroupLevel)
                <td>@Html.Raw(Model.Detail.Manufacturer)</td>
                <td>@Html.Raw(Model.Detail.Models)</td>
                <td>@Html.Raw(Model.Detail.Series)</td>
                <td>@Html.Raw(Model.Detail.BodyType)</td>
                <td>@Html.Raw(Model.Detail.Color)</td>
                <td>@Html.Raw(Model.Detail.StartPeriod == null ? "" : Model.Detail.StartPeriod.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(Model.Detail.EndPeriod == null ? "" : Model.Detail.EndPeriod.Value.ToString("dd-MMM-yyyy"))</td>
            </tr>
        }
    </tbody>
</table>