@model FMS.Website.Models.CsfItemModel

<table id="tbCsfVehicleVendor" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Police Number</th>
            <th>Manufacturer</th>
            <th>Model</th>
            <th>Series</th>
            <th>Body Type</th>
            <th>Vendor</th>
            <th>Color</th>
            <th>Start Contract</th>
            <th>End Contract</th>
            <th>PO Number</th>
            <th>Note</th>
        </tr>
    </thead>
    <tbody id="tb-body-select-vehicle-from-vendor">
        @if (Model.Detail.ModelsVendor != null)
        {
            <tr>
                @Html.HiddenFor(model => model.Detail.PoliceNumberVendor)
                @Html.HiddenFor(model => model.Detail.ManufacturerVendor)
                @Html.HiddenFor(model => model.Detail.ModelsVendor)
                @Html.HiddenFor(model => model.Detail.SeriesVendor)
                @Html.HiddenFor(model => model.Detail.BodyTypeVendor)
                @Html.HiddenFor(model => model.Detail.VendorNameVendor)
                @Html.HiddenFor(model => model.Detail.ColorVendor)
                @Html.HiddenFor(model => model.Detail.StartPeriodVendor)
                @Html.HiddenFor(model => model.Detail.EndPeriodVendor)
                @Html.HiddenFor(model => model.Detail.PoNumberVendor)
                @Html.HiddenFor(model => model.Detail.ChasisNumberVendor)
                @Html.HiddenFor(model => model.Detail.EngineNumberVendor)
                @Html.HiddenFor(model => model.Detail.IsAirBagVendor)
                @Html.HiddenFor(model => model.Detail.TransmissionVendor)
                @Html.HiddenFor(model => model.Detail.BrandingVendor)
                @Html.HiddenFor(model => model.Detail.PurposeVendor)
                @Html.HiddenFor(model => model.Detail.PoLineVendor)
                @Html.HiddenFor(model => model.Detail.IsVatVendor)
                @Html.HiddenFor(model => model.Detail.IsRestitutionVendor)
                <td>
                    @Html.Raw(Model.Detail.PoliceNumberVendor)
                </td>
                <td>@Html.Raw(Model.Detail.ManufacturerVendor)</td>
                <td>@Html.Raw(Model.Detail.ModelsVendor)</td>
                <td>@Html.Raw(Model.Detail.SeriesVendor)</td>
                <td>@Html.Raw(Model.Detail.BodyTypeVendor)</td>
                <td>@Html.Raw(Model.Detail.VendorNameVendor)</td>
                <td>@Html.Raw(Model.Detail.ColorVendor)</td>
                <td>@Html.Raw(Model.Detail.StartPeriodVendor == null ? "" : Model.Detail.StartPeriodVendor.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(Model.Detail.EndPeriodVendor == null ? "" : Model.Detail.EndPeriodVendor.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(Model.Detail.PoNumberVendor)</td>
                <td>@Html.Raw(Model.Detail.CommentsVendor)</td>
            </tr>
        }
    </tbody>
</table>